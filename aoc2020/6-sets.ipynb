    "c = 0\n",
    "for i in groups:\n",
    "    p = i.split('\\n')\n",
    "    a = set()\n",
    "    for ki,k in enumerate(p):\n",
    "        b = set()\n",
    "        for j in k:\n",
    "            b.add(j)\n",
    "        a = b if ki == 0 else a.intersection(b)\n",
    "    c += len(a)\n",
    "print(c)"
